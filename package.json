{
  "private": true,
  "name": "nhakhoaphuocbinh",
  "homepage": "http://nhakhoaphuocbinh.com",
  "scripts": {
    "lint:js": "eslint --fix .",
    "lint:css": "stylelint \"web_modules/**/*.css\"",
    "lint": "npm run lint:js && npm run lint:css",
    "start": "statinamic start",
